{
  "snippets.ultisnips.pythonPrompt": false,
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": ["--skip-string-normalization"],
  "python.linting.ruffEnabled": true,
  "python.linting.ruffArgs": ["--line-length=150", "--preview"],
  "rust-analyzer.check.command": "clippy",
  "coc.preferences.extensionUpdateCheck": "daily",
  "sql.formatOptions": {
    "language": "tsql",
    "tabWidth": 4,
    "keywordCase": "upper",
    "dataTypeCase": "upper",
    "functionCase": "upper",
    "logicalOperatorNewline": true,
    "expressionWidth": 30,
    "denseOperators": true
  },
  "svelte.enable-ts-plugin": false,
  "languageserver": {
      "csharp-ls": {
          "command": "csharp-ls",
          "filetypes": ["cs"],
          "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
      }
  },
  "csharp.formatting.provider": "format"
}
